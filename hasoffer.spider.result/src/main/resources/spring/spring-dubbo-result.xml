<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       default-lazy-init="false">
    <dubbo:application name="spider-consumer"/>
    <!-- 使用zookeeper注册中心暴露服务地址 -->
    <dubbo:registry address="zookeeper://zookeeper_server:2181" check="false"/>

    <dubbo:protocol name="dubbo" port="20880"/>

    <!--<dubbo:provider timeout="5000"/>-->

    <!-- 要暴露的服务接口 -->
    <dubbo:service interface="hasoffer.dubbo.spider.result.api.ISkuResultDubboService" version="1.0"
                   ref="skuResultDubboService" timeout="10000" />
    <bean id="skuResultDubboService" class="hasoffer.spider.result.service.impl.SkuResultDubboServiceImpl"/>

    <dubbo:service interface="hasoffer.dubbo.spider.result.api.IProductResultDubboService" version="1.0"
                   ref="productResultDubboService" timeout="10000" />
    <bean id="productResultDubboService" class="hasoffer.spider.result.service.impl.ProductResultDubboServiceImpl"/>

</beans>